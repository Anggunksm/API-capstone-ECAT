{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome",
  "definitions": {
    "Welcome": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "meta": {
          "$ref": "#/definitions/Meta"
        },
        "results": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Result"
          }
        }
      },
      "required": [
        "meta",
        "results"
      ],
      "title": "Welcome"
    },
    "Meta": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        },
        "page": {
          "type": "integer"
        },
        "limit": {
          "type": "integer"
        },
        "total": {
          "type": "integer"
        }
      },
      "required": [
        "limit",
        "message",
        "page",
        "success",
        "total"
      ],
      "title": "Meta"
    },
    "Result": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "adminId": {
          "type": "string"
        },
        "category": {
          "type": "string"
        },
        "fullname": {
          "type": "string"
        },
        "photoImage": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "content": {
          "type": "string"
        },
        "date": {
          "type": "string",
          "format": "date-time"
        },
        "imageUrl": {
          "type": "string"
        },
        "feedback": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Feedback"
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "likes": {
          "anyOf": [
            {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Like"
              }
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": [
        "adminId",
        "category",
        "content",
        "date",
        "feedback",
        "fullname",
        "id",
        "imageUrl",
        "likes",
        "photoImage",
        "title"
      ],
      "title": "Result"
    },
    "Feedback": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "fullname": {
          "$ref": "#/definitions/Fullname"
        },
        "role": {
          "$ref": "#/definitions/Role"
        },
        "photoImage": {
          "type": "string"
        },
        "newsId": {
          "$ref": "#/definitions/NewsID"
        },
        "content": {
          "type": "string"
        }
      },
      "required": [
        "content",
        "fullname",
        "id",
        "newsId",
        "photoImage",
        "role"
      ],
      "title": "Feedback"
    },
    "Like": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "userId": {
          "type": "string"
        },
        "newsId": {
          "$ref": "#/definitions/NewsID"
        },
        "status": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "newsId",
        "status",
        "userId"
      ],
      "title": "Like"
    },
    "Fullname": {
      "type": "string",
      "enum": [
        "Naufal",
        "user12",
        "DY",
        "anisa",
        "Rasyid"
      ],
      "title": "Fullname"
    },
    "NewsID": {
      "type": "string",
      "enum": [
        "z8Yui2",
        "8OXPhR"
      ],
      "title": "NewsID"
    },
    "Role": {
      "type": "string",
      "enum": [
        "user"
      ],
      "title": "Role"
    }
  }
}
