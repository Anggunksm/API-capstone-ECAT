{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome",
  "definitions": {
    "Welcome": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "meta": {
          "$ref": "#/definitions/Meta"
        },
        "results": {
          "$ref": "#/definitions/Results"
        }
      },
      "required": [
        "meta",
        "results"
      ],
      "title": "Welcome"
    },
    "Meta": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "success": {
          "type": "boolean"
        },
        "message": {
          "type": "string"
        }
      },
      "required": [
        "message",
        "success"
      ],
      "title": "Meta"
    },
    "Results": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "adminId": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "content": {
          "type": "string"
        },
        "date": {
          "type": "string",
          "format": "date-time"
        },
        "feedback": {
          "type": "null"
        },
        "likes": {
          "type": "null"
        }
      },
      "required": [
        "adminId",
        "content",
        "date",
        "feedback",
        "id",
        "likes",
        "title"
      ],
      "title": "Results"
    }
  }
}
